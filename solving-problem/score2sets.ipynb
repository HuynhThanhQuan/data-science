   "source": [
    "# BASE CASE:\n",
    "# 2 empty lists, score = 1\n",
    "# GENERAL CASE:\n",
    "# score = AND/OR\n",
    "def score2sets(set1, set2):\n",
    "    _AND = set(set1) & set(set2)\n",
    "    _OR = set(set1) | set(set2)\n",
    "    if len(_OR) >= 1:\n",
    "        return len(_AND) / len(_OR)\n",
    "    return 1"
   ]
